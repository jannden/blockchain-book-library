{
  "name": "nft-marketplace-frontend",
  "author": "Jan Cibulka",
  "version": "1.0.0",
  "description": "A Next.js frontend for interacting with the NFT Marketplace and NFT Collection smart contracts, allowing users to mint, list, buy, and sell NFTs seamlessly.",
  "url": "https://github.com/jannden/nft-marketplace",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev -p 4001",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "compile-contract-types": "typechain --target ethers-v5 --out-dir contracts/types './contracts/*.json'",
    "postinstall": "npm run compile-contract-types"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/units": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "@fontsource/roboto": "^5.0.13",
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/onboarding": "^1.0.1",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "graphql": "^16.8.1",
    "ipfs-http-client": "^56.0.3",
    "next": "^14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.23.1",
    "swr": "^2.2.5",
    "urql": "^4.1.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^11.1.2",
    "@types/node": "^20.12.13",
    "@types/react": "^18.3.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "ethers": "^5.7.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.5"
  }
}